{
  "name": "discord_music_player",
  "version": "1.0.0",
  "private": true,
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node --loader ts-node/esm src/main.ts",
    "dev": "nodemon -I --exec node --experimental-specifier-resolution=node  --loader ts-node/esm ./src/main.ts",
    "build": "rimraf dist && tsc -p tsconfig.build.json && yarn postbuild",
    "postbuild": "yarn run copyfiles -f src/**/*.html dist/ && yarn run copyfiles assets/**/* dist/",
    "prepublishOnly": "yarn build",
    "release": "standard-version",
    "release:first": "standard-version --first-release",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --verbose false"
  },
  "keywords": [],
  "author": "ChristopherVR",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@discord-player/equalizer": "^0.2.3",
    "@discord-player/extractor": "^4.4.5",
    "@discordjs/opus": "^0.9.0",
    "axios": "^1.6.0",
    "copyfiles": "^2.4.1",
    "discord-player": "^6.6.6",
    "discord.js": "^14.13.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.3",
    "ffmpeg-static": "^5.2.0",
    "i18next": "^23.6.0",
    "i18next-http-backend": "^2.3.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "openai": "^4.14.1",
    "pino": "^8.16.1",
    "pino-pretty": "^10.2.3",
    "youtube-sr": "^4.3.9",
    "ytdl-core": "^4.11.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@eslint/js": "^8.53.0",
    "@types/express": "^4.17.20",
    "@types/node": "^20.8.9",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsdoc": "^46.8.2",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.2.2"
  }
}
