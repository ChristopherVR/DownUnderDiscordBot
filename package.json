{
  "name": "discord_music_player",
  "version": "1.0.0",
  "private": true,
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node --loader ts-node/esm src/main.ts",
    "dev": "nodemon -I --exec node --experimental-specifier-resolution=node  --loader ts-node/esm ./src/main.ts",
    "build": "rimraf dist && tsc -p tsconfig.build.json && yarn postbuild",
    "postbuild": "yarn run copyfiles -f src/**/*.html dist/ && yarn run copyfiles assets/**/* dist/",
    "prepublishOnly": "yarn build",
    "release": "standard-version",
    "release:first": "standard-version --first-release",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --verbose false"
  },
  "keywords": [],
  "author": "ChristopherVR",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@discord-player/equalizer": "^7.1.0",
    "@discord-player/extractor": "^7.1.0",
    "@discordjs/opus": "^0.10.0",
    "axios": "^1.8.4",
    "bgutils-js": "^3.2.0",
    "copyfiles": "^2.4.1",
    "discord-player": "^7.1.0",
    "discord-player-youtubei": "^1.4.3",
    "discord.js": "^14.18.0",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "ffmpeg-static": "^5.2.0",
    "i18next": "^24.2.3",
    "i18next-http-backend": "^3.0.2",
    "jest": "^29.7.0",
    "mediaplex": "^1.0.0",
    "nodemon": "^3.1.9",
    "openai": "^4.91.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "youtube-sr": "^4.3.11",
    "ytdl-core": "^4.11.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.15.0",
    "@types/express": "^5.0.1",
    "@types/node": "^22.13.14",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^4.3.1",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.9",
    "eslint-plugin-n": "^17.17.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-promise": "^7.2.1",
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.13",
    "typescript": "^5.8.2"
  }
}
