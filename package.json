{
    "name": "discord_music_player",
    "version": "1.0.0",
    "private": true,
    "main": "dist/main.js",
    "type": "module",
    "scripts": {
        "start": "node --experimental-specifier-resolution=node --loader ts-node/esm src/main.ts",
        "dev": "nodemon -I --exec node --experimental-specifier-resolution=node  --loader ts-node/esm ./src/main.ts",
        "build": "rimraf dist && tsc -p tsconfig.build.json && yarn postbuild",
        "postbuild": "yarn run copyfiles -f src/**/*.html dist/ && yarn run copyfiles assets/**/* dist/",
        "prepublishOnly": "yarn build",
        "release": "standard-version",
        "release:first": "standard-version --first-release",
        "test": "jest --passWithNoTests",
        "test:watch": "jest --watch --verbose false"
    },
    "keywords": [],
    "author": "ChristopherVR",
    "license": "ISC",
    "description": "",
    "dependencies": {
        "@discord-player/equalizer": "^0.2.3",
        "@discord-player/extractor": "^4.5.1",
        "@discordjs/opus": "^0.9.0",
        "axios": "^1.7.7",
        "copyfiles": "^2.4.1",
        "discord-player": "^6.7.1",
        "discord-player-youtubei": "^1.3.5",
        "discord.js": "^14.16.3",
        "dotenv": "^16.4.5",
        "express": "^4.21.1",
        "express-rate-limit": "^7.4.1",
        "ffmpeg-static": "^5.2.0",
        "i18next": "^24.0.0",
        "i18next-http-backend": "^3.0.1",
        "jest": "^29.7.0",
        "mediaplex": "^0.0.9",
        "nodemon": "^3.1.7",
        "openai": "^4.73.0",
        "pino": "^9.5.0",
        "pino-pretty": "^13.0.0",
        "youtube-sr": "^4.3.11",
        "ytdl-core": "^4.11.5"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.6.0",
        "@commitlint/config-conventional": "^19.6.0",
        "@eslint/js": "^9.15.0",
        "@types/express": "^5.0.0",
        "@types/node": "^22.9.3",
        "@typescript-eslint/eslint-plugin": "^8.15.0",
        "@typescript-eslint/parser": "^8.15.0",
        "eslint": "^8.57.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-import-resolver-typescript": "^3.6.3",
        "eslint-plugin-deprecation": "^3.0.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-jsdoc": "^50.5.0",
        "eslint-plugin-n": "^17.14.0",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-promise": "^7.1.0",
        "husky": "^9.1.7",
        "prettier": "^3.3.3",
        "rimraf": "^6.0.1",
        "ts-node": "^10.9.2",
        "tsc-alias": "^1.8.10",
        "typescript": "^5.7.2"
    }
}
