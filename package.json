{
  "name": "discord_music_player",
  "version": "1.0.0",
  "private": true,
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node --loader ts-node/esm src/main.ts",
    "dev": "nodemon -I --exec node --experimental-specifier-resolution=node  --loader ts-node/esm ./src/main.ts",
    "build": "rimraf dist && tsc -p tsconfig.build.json && yarn postbuild",
    "postbuild": "yarn run copyfiles -f src/**/*.html dist/ && yarn run copyfiles assets/**/* dist/",
    "prepublishOnly": "yarn build",
    "release": "standard-version",
    "release:first": "standard-version --first-release",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --verbose false"
  },
  "keywords": [],
  "author": "ChristopherVR",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@discord-player/equalizer": "^0.2.3",
    "@discord-player/extractor": "^4.5.0",
    "@discordjs/opus": "^0.9.0",
    "axios": "^1.7.2",
    "copyfiles": "^2.4.1",
    "discord-player": "^6.7.0",
    "discord-player-youtubei": "^1.2.0",
    "discord.js": "^14.15.3",
    "dotenv": "^16.3.1",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "ffmpeg-static": "^5.2.0",
    "i18next": "^23.12.2",
    "i18next-http-backend": "^2.5.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "openai": "^4.53.1",
    "pino": "^9.3.2",
    "pino-pretty": "^11.2.2",
    "youtube-sr": "^4.3.11",
    "ytdl-core": "^4.11.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.7.0",
    "@types/express": "^4.17.20",
    "@types/node": "^20.14.12",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsdoc": "^48.8.3",
    "eslint-plugin-n": "^17.10.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.0.0",
    "husky": "^9.1.2",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.5.4"
  }
}
